  Application.MainFormOnTaskbar := True;
  if (Mutex <> 0) then
  begin
    frmSplashScreen:=TfrmSplashScreen.Create(Application);
    frmSplashScreen.Show;
    Application.Initialize;
    Application.Title := 'Cliente Servicios (Utilerias) 1.0';
